  "keywords": [
    "security",
    "pipeline",
    "nodejs",
    "aws",
    "lambda",
    "container"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "retire": "^4.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
